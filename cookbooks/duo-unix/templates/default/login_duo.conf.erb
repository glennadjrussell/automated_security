[duo]
; Duo integration key
ikey = <%= @ikey %>
; Duo secret key
skey = <%= @skey %>
; Duo API host
host = <%= @host %>
<% if @group %>
; UNIX group that does 2-factor
group = <%= @group %>
<% end -%>
; Failure mode
failmode = <%= @failmode %>
; Send command for Duo Push authentication
pushinfo = <%= @pushinfo %>
